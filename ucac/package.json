{
  "name": "lndr",
  "version": "1.0.0",
  "description": "UCAC-based venmo clone",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "migrate": "truffle migrate --network testrpc",
    "ganache": "ganache-cli --blocktime 1 -e 100000000000000000000 -m gravity top burden flip student usage spell purchase hundred improve check genre > /dev/null 2>&1 & echo $!",
    "compile": "truffle compile",
    "migrate-ganache": "truffle migrate --network ganache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockmason/lndr.git"
  },
  "keywords": [
    "ethereum"
  ],
