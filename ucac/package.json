    "url": "https://github.com/blockmason/lndr/issues"
  },
  "homepage": "https://github.com/blockmason/lndr#readme",
  "devDependencies": {
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chai-bignumber": "2.0.2",
    "credit-protocol": "https://github.com/blockmason/credit-protocol.git#604f885de0b2f8f585e3925bce940b1104749a21",
    "ganache-cli": "6.1.3",
    "tce-contracts": "https://github.com/blockmason/tce-contracts.git#bfbf5e87577cecd7a06ed75cecfc52e7b1d90d77",
